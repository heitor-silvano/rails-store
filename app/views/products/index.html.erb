<h1>Products#index</h1>
<h1><%= t ".title" %></h1>

<%= link_to "[Novo produto]", new_product_path if authenticated? %>

<%= link_to "[Login]", new_session_path unless authenticated? %>

<div>
  <% @products.each do |product| %>
  <div>
    <%= link_to product.name, product_path(product.id) %>

    <%= product.created_at %>
  </div>
  <% end %>
</div>